msgid_plural "There are %i hatches in the floor."
msgstr[0] "W podłodze jest %i właz."
msgstr[1] "W podłodze jest %i włazy."
msgstr[2] "W podłodze jest %i włazów."

#: src/skarabeusz.cc:605
#, c-format
msgid "The dwarf says: \"I would give you the %s key for your keys.\""
msgstr "Krasnolud mówi: \"Dam Ci klucz %s w zamian za Twoje klucze.\""

#: src/skarabeusz.cc:624 src/skarabeusz.cc:704
#, c-format
msgid "%s and %s"
msgstr "%s i %s"

#: src/skarabeusz.cc:627 src/skarabeusz.cc:658
#, c-format
msgid "The dwarf says: \"I would give you the keys %s for your keys.\""
msgstr "Krasnolud mówi: \"Dam Ci klucze %s w zamian za Twoje klucze.\""

#: src/skarabeusz.cc:686
#, c-format
msgid "You have the %s key."
msgstr "Masz przy sobie %s klucz."

#: src/skarabeusz.cc:707
#, c-format
msgid "You have the keys %s."
msgstr "Masz przy sobie klucze %s."

#: src/skarabeusz.cc:737
#, c-format
msgid "You have the following keys: %s."
msgstr "Masz przy sobie następujące klucze: %s."

#: src/skarabeusz.cc:758
msgid "The dwarf pays no attention at you."
msgstr "Krasnolud nie zwraca na Ciebie uwagi."

#: src/skarabeusz.cc:768
#, c-format
msgid "You are talking with the dwarf at %s."
msgstr "Rozmawiasz z krasnoludem w %s."

#: src/skarabeusz.cc:922
msgid "What do you want to do?"
msgstr "Co chcesz zrobić?"

#: src/skarabeusz.cc:923
msgid "Talk with the dwarf - go to "
msgstr "Porozmawiaj z krasnoludem - idź do "

#: src/skarabeusz.cc:949
#, c-format
msgid "Go to the field %s - "
msgstr "Idź na pole %s - "

#: src/skarabeusz.cc:955
#, c-format
msgid "You are standing in front of the door at %s - "
msgstr "Stoisz przed drzwiami na %s."

#: src/skarabeusz.cc:972
#, c-format
msgid "You can open the door %s and go north "
msgstr "Możesz otworzyć drzwi %s i iść na północ "

#: src/skarabeusz.cc:978
msgid "None of your keys opens the north door."
msgstr "Żaden z Twoich kluczy nie otwiera północnych drzwi."

#: src/skarabeusz.cc:984
#, c-format
msgid "You can open the door %s and go east "
msgstr "Możesz otworzyć drzwi %s i iść na wschód "

#: src/skarabeusz.cc:990
msgid "None of your keys opens the east door."
msgstr "Żaden z Twoich kluczy nie otwiera wschodnich drzwi."

#: src/skarabeusz.cc:996
#, c-format
msgid "You can open the door %s and go south "
msgstr "Możesz otworzyć drzwi %s i iść na południe "

#: src/skarabeusz.cc:1002
msgid "None of your keys opens the south door."
msgstr "Żaden z Twoich kluczy nie otwiera południowych drzwi."

#: src/skarabeusz.cc:1008
#, c-format
msgid "You can open the door %s and go west "
msgstr "Możesz otworzyć drzwi %s i iść na zachód "

#: src/skarabeusz.cc:1014
msgid "None of your keys opens the west door."
msgstr "Żaden z Twoich kluczy nie otwiera zachodnich drzwi."

#: src/skarabeusz.cc:1020
#, c-format
msgid "You can open the hatch %s and descend to the lower level "
msgstr "Możesz otworzyć właz %s i zejść na niższy poziom "

#: src/skarabeusz.cc:1026
msgid "None of your keys opens the hatch in the floor."
msgstr "Żaden z Twoich kluczy nie otwiera włazu w podłodze."

#: src/skarabeusz.cc:1032
#, c-format
msgid "You can open the hatch %s and ascend to the upper level "
msgstr "Możesz otworzyć właz %s i wejść na wyższy poziom "

#: src/skarabeusz.cc:1038
msgid "None of your keys opens the hatch in the ceiling."
msgstr "Żaden z Twoich kluczy nie otwiera włazu w suficie."

#: src/skarabeusz.cc:1045 src/skarabeusz.cc:1109
msgid "You can look around "
msgstr "Możesz się rozejrzeć "

#: src/skarabeusz.cc:1070
#, c-format
msgid "You are standing next to the dwarf at %s."
msgstr "Stoisz obok krasnoluda w %s."

#: src/skarabeusz.cc:1077
msgid "Hello, my name is "
msgstr "Witaj, mam na imię "

#: src/skarabeusz.cc:1083
msgid "Congratulations! You have won!"
msgstr "Gratulacje! Wygrałeś!"

#: src/skarabeusz.cc:1097
msgid "If you accept it go to "
msgstr "Jeśli się zgadzasz idź do "

#: src/skarabeusz.cc:1105
msgid "The dwarf does not accept any exchange."
msgstr "Krasnolud nie zgadza się na żadną wymianę."

#: src/skarabeusz.cc:2229
#, c-format
msgid "This is a map of the level %i."
msgstr "To jest mapa poziomu %i-go."

#: src/skarabeusz.cc:2277
msgid "teleport"
msgstr "teleport"

#: src/skarabeusz.cc:2281
#, c-format
msgid "Hello! Press %s in order to enter the maze."
msgstr "Witaj! Naciśnij %s aby wejść do labiryntu."
