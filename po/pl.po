msgstr[2] "W podłodze jest %i włazów."

#: src/skarabeusz.cc:1366
#, c-format
msgid "The dwarf says: \"I would give you the %s key for your keys.\""
msgstr "Krasnolud mówi: \"Dam Ci klucz %s w zamian za Twoje klucze.\""

#: src/skarabeusz.cc:1385 src/skarabeusz.cc:1560
#, c-format
msgid "%s and %s"
msgstr "%s i %s"

#: src/skarabeusz.cc:1388 src/skarabeusz.cc:1419
#, c-format
