#: src/skarabeusz.cc:835
msgid "You can ascend to the upper level "
msgstr "Możesz wejść na wyższy poziom "

#: src/skarabeusz.cc:824
msgid "You can descend to the lower level "
msgstr "Możesz zejść na niższy poziom "

#: src/skarabeusz.cc:847 src/skarabeusz.cc:911
msgid "You can look around "
msgstr "Możesz się rozejrzeć "

#: src/skarabeusz.cc:791
msgid "You can open the door and go east "
msgstr "Możesz otworzyć drzwi i iść na wschód "

#: src/skarabeusz.cc:780
msgid "You can open the door and go north "
msgstr "Możesz otworzyć drzwi i iść na północ "

#: src/skarabeusz.cc:802
msgid "You can open the door and go south "
msgstr "Możesz otworzyć drzwi i iść na południe "

#: src/skarabeusz.cc:813
msgid "You can open the door and go west "
msgstr "Możesz otworzyć drzwi i iść na zachód "

